stings == null ? html`\r\n            <sl-spinner></sl-spinner>\r\n          ` : html`\r\n            ${this.favListings.map(listing => html`\r\n              <va-listing-card class=\"listing-card\"\r\n                id=\"${listing._id}\"\r\n                user=\"${JSON.stringify(listing.user)}\"\r\n                image=\"${listing.image}\"\r\n                location=\"${listing.location}\"\r\n                level=\"${listing.level}\"\r\n                certified=\"${listing.certified}\"\r\n                essenceStatement=\"${listing.essenceStatement}\"\r\n                classType=\"${listing.classType}\"\r\n                gender=\"${listing.gender}\"\r\n              >        \r\n              </va-listing-card>\r\n\r\n            `)}\r\n          `}\r\n        </div>\r\n        \r\n      </div>      \r\n    `\r\n    render(template, App.rootEl)\r\n  }\r\n}\r\n\r\n\r\nexport default new FavouritesView()"},"lineCount":null}},"error":null,"hash":"6c25f7534581cd42b007423a65cd2f5d","cacheData":{"env":{}}}