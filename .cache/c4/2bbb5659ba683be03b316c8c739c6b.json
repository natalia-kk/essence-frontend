":"addFavHandler","generated":{"line":100,"column":240},"source":"components/va-listing-card.js","original":{"line":108,"column":82}},{"generated":{"line":100,"column":253},"source":"components/va-listing-card.js","original":{"line":108,"column":77}},{"name":"bind","generated":{"line":100,"column":254},"source":"components/va-listing-card.js","original":{"line":108,"column":96}},{"generated":{"line":100,"column":258},"source":"components/va-listing-card.js","original":{"line":108,"column":77}},{"generated":{"line":100,"column":259},"source":"components/va-listing-card.js","original":{"line":108,"column":101}},{"generated":{"line":100,"column":263},"source":"components/va-listing-card.js","original":{"line":108,"column":77}},{"generated":{"line":100,"column":264},"source":"components/va-listing-card.js","original":{"line":81,"column":4}},{"generated":{"line":101,"column":0},"source":"components/va-listing-card.js","original":{"line":112,"column":3}},{"generated":{"line":101,"column":3},"source":"components/va-listing-card.js","original":{"line":18,"column":74}},{"generated":{"line":101,"column":4},"source":"components/va-listing-card.js","original":{"line":112,"column":4}},{"generated":{"line":104,"column":0},"source":"components/va-listing-card.js","original":{"line":18,"column":74}},{"generated":{"line":104,"column":1},"source":"components/va-listing-card.js","original":{"line":18,"column":0}}],"sources":{"components/va-listing-card.js":"// CUSTOM WEB COMPONENT\r\n\r\nimport { LitElement, html, css } from '@polymer/lit-element'\r\nimport {anchorRoute, gotoRoute} from './../Router' // required for navigating to any page in the SPA - brought in as dependency in case it's needed\r\nimport Auth from './../Auth' // in case need to access current user\r\nimport App from './../App' // in case need to access anything in the app\r\nimport UserAPI from './../UserAPI'\r\nimport Toast from './../Toast'\r\n\r\n\r\n// DEFINE THE CUSTOM ELEMENT --------------------------\r\n// window object -> customElements -> method called 'define'\r\n    /* define = takes 2 parameters: \r\n        1- name of the custom html tag (va-listing-card) \r\n        2- the class we want to connect it to (class is created here) \r\n         - extending LitElement - required because class is created after (not using an already made class) */\r\n\r\ncustomElements.define('va-listing-card', class Listing extends LitElement {\r\n  constructor(){\r\n    super()    \r\n  }\r\n\r\n // DEFINE PROPERTIES (custom tag attributes)\r\n // attributes are set inside the tag in each view it is used\r\n  static get properties(){\r\n    return {\r\n      id: {\r\n        type: String\r\n      },\r\n      user: {\r\n        type: Object\r\n      },\r\n      image: {\r\n        type: String \r\n      },\r\n      level: {\r\n          type: String\r\n      },\r\n      location: {\r\n        type: String,\r\n      }, \r\n      certified: {\r\n        type: String\r\n      }, \r\n      essenceStatement: {\r\n        type: String\r\n      }, \r\n      gender: {\r\n        type: String\r\n      },\r\n      classType: {\r\n        type: String\r\n      }   \r\n    } \r\n  }\r\n\r\n  // firstUpdated function\r\n  firstUpdated() {\r\n    super.firstUpdated()\r\n  }\r\n\r\n  // called when the See More button is clicked\r\n  viewListingHandler() {\r\n    gotoRoute(`/listing?id=${this.id}`)\r\n  } \r\n\r\n  // called when add to favourties heart button/icon is clicked\r\n  // linked to UserAPI (backend) to add the listing to User model for current user.\r\n  async addFavHandler(){    \r\n    try {\r\n      await UserAPI.addFavListing(this.id)\r\n      Toast.show('Listing added to favourites')\r\n    }catch(err){\r\n      Toast.show(err, 'error')\r\n    }\r\n  }\r\n  \r\n  // RENDER THE ELEMENT - html and css\r\n  // shadow root\r\n  render() {    \r\n    return html`\r\n    <style>\r\n      h2 {\r\n        text-transform: uppercase;\r\n        \r\n      }\r\n      h3 {\r\n        color: var(--brand-color);\r\n      }\r\n      h4 {\r\n        color: grey;\r\n        font-weight: 550;\r\n      }\r\n      sl-icon-button {\r\n        float: right;\r\n        font-size: 1.5em;\r\n      }\r\n    </style>\r\n\r\n    <sl-card>\r\n        <img class=\"listing-card-image\" slot=\"image\" src=\"${App.apiBase}/images/${this.image}\">\r\n        <h2 class=\"author\">${this.user.firstName} ${this.user.lastName} </h2>\r\n        <h3>${this.location}</h3>\r\n        <h4>${this.classType} classes | ${this.level}</h4>\r\n        <p>${this.certified}hr certified | ${this.gender}</p>\r\n\r\n        <sl-button @click=${this.viewListingHandler.bind(this)}>See More ...</sl-button>\r\n        <sl-icon-button name=\"suit-heart\" label=\"Add to Favourites\" @click=${this.addFavHandler.bind(this)}></sl-icon-button>\r\n    </sl-card>\r\n   \r\n    `\r\n  } // end render()\r\n  \r\n})"},"lineCount":null}},"error":null,"hash":"ad9a263ef8408220175741f06da14c52","cacheData":{"env":{}}}